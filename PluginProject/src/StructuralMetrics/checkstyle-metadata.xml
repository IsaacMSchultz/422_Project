<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="Plugin Project"  priority="1600">
       <description>%testPlugin.desc</description>

        <rule-metadata name="CyclomaticComplexityCounter" internal-name="CyclomaticComplexityCounter" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.CyclomaticComplexityCounter" />
            <description>%ACyclomaticComplexity.desc</description>
        </rule-metadata>
        
        <rule-metadata name="VariableCountCheck" internal-name="VariableCountCheck" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.VariableCountCheck" />
            <description>%VariableCountCheck.desc</description>
        </rule-metadata>
        
        <rule-metadata name="ExpressionCountCheck" internal-name="ExpressionCountCheck" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.ExpressionCountCheck" />
            <description>%ExpressionCountCheck.desc</description>
        </rule-metadata>
        
        <rule-metadata name="NumberOfCastsCheck" internal-name="NumberOfCastsCheck" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.NumberOfCastsCheck" />
            <description>%NumberOfCasts.desc</description>
        </rule-metadata>

        <rule-metadata name="Comments" internal-name="Comments" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.CommentsCheck" />
            <description>%Comments.desc</description>
             <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed comments.</description>
            </property-metadata>
        </rule-metadata>
        
    </rule-group-metadata>
</checkstyle-metadata>
