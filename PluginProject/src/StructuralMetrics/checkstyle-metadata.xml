<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="PluginProject"  priority="1600">
        <description>%test.desc</description>
        <rule-metadata name="AntiHungarianDemo" internal-name="AntiHungarianDemo" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.AntiHungarianDemoCheck" />
            <description>%AntiHungarianDemoCheck.desc</description>
        </rule-metadata>
        <rule-metadata name="Comments" internal-name="Comments" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.CommentsCheck" />
            <description>%Comments.desc</description>
             <property-metadata name="max" datatype="Integer" default-value="5">
                <description>The maximum number of allowed comments.</description>
            </property-metadata>
        </rule-metadata>
        <rule-metadata name="LinesOfComments" internal-name="LinesOfComments" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.LinesOfCommentsCheck" />
            <description>%LinesOfCommentsCheck.desc</description>
             <property-metadata name="max" datatype="Integer" default-value="5">
                <description>The maximum number of comment lines.</description>
            </property-metadata>
        </rule-metadata>
        <rule-metadata name="MaxNumLoops" internal-name="MaxNumLoops" parent="TreeWalker">
            <alternative-name internal-name="StructuralMetrics.MaxNumLoopsCheck" />
            <description>%MaxNumLoopsCheck.desc</description>
             <property-metadata name="max" datatype="Integer" default-value="1">
                <description>The maximum number of loop statements.</description>
            </property-metadata>
        </rule-metadata>
    </rule-group-metadata>
</checkstyle-metadata>
